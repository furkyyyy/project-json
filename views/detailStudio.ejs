<%- include("partials/navbar") %>

<main class="flex-grow-1 container mt-5 text-center">
    <img src="<%= studio.logoUrl %>" alt="<%= studio.name %>" class="img-fluid mb-4" style="max-height: 200px;">

    <h2><%= studio.name %></h2>
    <p><strong>Founded Year:</strong> <%= studio.foundedYear %></p>
    <p><strong>Founder:</strong> <%= studio.founder %></p>
